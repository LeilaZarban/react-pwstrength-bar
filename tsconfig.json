{
    "compilerOptions": {
      "outDir": "./lib/",
      "noImplicitAny": true,
      "module": "ES2020",
      "moduleResolution": "node",
      "target": "ES2020",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "plugins": [ { "name": "typescript-plugin-css-modules" } ],
    },
    "exclude": [
      "Scripts/**",
      "./lib/"
    ]
  }